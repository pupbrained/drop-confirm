modLoader = "kotlinforforge"
loaderVersion = "[4.10,)"
issueTrackerURL = "${mod_issue_tracker}"
license = "${mod_license}"

[[mods]]
modId = "${mod_id}"
version = "${mod_version}"
displayName = "${mod_name}"
description = '''${mod_description}'''
authors = "${mod_author}"
logoFile = "drop_confirm.png"

[[dependencies.${mod_id}]]
modId = "forge"
versionRange = "[1,)"
mandatory = true
ordering = "NONE"
side = "BOTH"

[[dependencies.${mod_id}]]
modId = "minecraft"
versionRange = "[1.14,)"
mandatory = true

[[dependencies.${mod_id}]]
